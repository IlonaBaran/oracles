{
    "source": {
      "include": [
        "src"
      ],
      "includePattern": ".+\\.(vue|js)$"
    },
    "plugins": ["plugins/markdown"],
    "opts": {
      "recurse": true,
      "destination": "docs",
      "readme": "README.md"
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "markdown": {
      "idInHeadings": true
    }
  }
  